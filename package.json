{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "_e2e_testprep": "webpack --config webpack.e2e-test.js",
    "_cleanup_frontend_cov": "rm -rf coverage/frontend/*",
    "_gen_frontend_cov": "nyc report -t coverage/frontend -r text -r html --report-dir coverage/frontend",
    "test_frontend": "jest",
    "test_backend": "pytest tests/backend",
    "test_e2e": "npm run _e2e_testprep && pytest tests/end-to-end",
    "test": "npm run test_backend && npm run test_frontend && npm run test_e2e",
    "coverage_frontend": "npm run _cleanup_frontend_cov && npm run test_frontend -- --coverage && GENERATE_E2E_COVERAGE=1 npm run test_e2e && npm run _gen_frontend_cov",
    "coverage_e2e_only": "npm run _cleanup_frontend_cov && GENERATE_E2E_COVERAGE=1 npm run test_e2e && npm run _gen_frontend_cov",
    "coverage_backend": "npm run test_backend -- --cov-report html:coverage/backend --cov-report term --cov=smartsexplore/",
    "profile_frontend": "python profiling/frontend_profiling.py",
    "gen_sphinx_apidoc": "sphinx-apidoc -o docs/modules smartsexplore/ -fMeT",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit"
  },
  "author": "Inken Fender, Pia Pl√ºmer, Simon Welker",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/vue": "^6.3.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "27.0.0",
    "babel-jest": "^27.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "chromedriver": "^89.0.0",
    "clipboard": "^2.0.7",
    "core-js": "^3.23.3",
    "css-loader": "^5.1.1",
    "d3": "^7.6.1",
    "d3-force-reuse": "^1.0.1",
    "d3-force-sampled": "^1.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^2.0.0",
    "eslint": "^8.24.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^9.8.0",
    "file-loader": "^6.2.0",
    "geckodriver": "^3.2.0",
    "jest": "^27.0.0",
    "jest-expect-message": "^1.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc": "^4.0.2",
    "lodash": "^4.17.21",
    "materialize-css": "^1.0.0-rc.2",
    "mini-css-extract-plugin": "^2.6.1",
    "nyc": "^15.1.0",
    "sass": "^1.54.9",
    "sass-loader": "^10.1.1",
    "selenium-webdriver": "^4.0.0-beta.2",
    "ssri": "^8.0.1",
    "style-loader": "^2.0.0",
    "vue": "^3.0.7",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "module": "smartsexplore/static/bundle.js"
}
