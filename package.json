{
  "name": "smartsexplore",
  "version": "1.0.0",
  "description": "Create a virtual environment for the webserver and activate it: ``` python3 -m virtualenv venv source venv/bin/activate ```",
  "private": true,
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "_e2e_testprep": "webpack --config webpack.e2e-test.js",
    "_cleanup_frontend_cov": "rm -rf coverage/frontend/*",
    "_gen_frontend_cov": "nyc report -t coverage/frontend -r text -r html --report-dir coverage/frontend",
    "test_frontend": "jest",
    "test_backend": "pytest tests/backend",
    "test_e2e": "npm run _e2e_testprep && pytest tests/end-to-end",
    "test": "npm run test_backend && npm run test_frontend && npm run test_e2e",
    "coverage_frontend": "npm run _cleanup_frontend_cov && npm run test_frontend -- --coverage && npm run _gen_frontend_cov",
    "coverage_e2e": "npm run _cleanup_frontend_cov && GENERATE_E2E_COVERAGE=1 npm run test_e2e && npm run _gen_frontend_cov",
    "coverage_backend": "npm run test_backend -- --cov-report html:coverage/backend --cov-report term --cov=smartsexplore/",
    "profile_frontend": "python profiling/frontend_profiling.py",
    "gen_sphinx_apidoc": "sphinx-apidoc -o docs/modules smartsexplore/ -fMeT",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js"
  },
  "author": "Inken Fender, Pia Pl√ºmer, Simon Welker",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/vue": "^6.3.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "chromedriver": "^89.0.0",
    "clipboard": "^2.0.7",
    "core-js": "^2.6.12",
    "css-loader": "^5.1.1",
    "d3-drag": "^2.0.0",
    "d3-force": "^2.1.1",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-zoom": "^2.0.0",
    "file-loader": "^6.2.0",
    "geckodriver": "^1.22.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^26.6.3",
    "jest-expect-message": "^1.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc": "^3.6.6",
    "lodash": "^4.17.21",
    "materialize-css": "^1.0.0-rc.2",
    "node-sass": "^5.0.0",
    "nyc": "^15.1.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "selenium-webdriver": "^4.0.0-beta.2",
    "ssri": "^8.0.1",
    "style-loader": "^2.0.0",
    "vue": "^3.0.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "module": "smartsexplore/static/bundle.js"
}
